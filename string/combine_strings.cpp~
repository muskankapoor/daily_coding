#include <iostream>
#include <string> 
using namespace std;


bool CombineStrings(string str1, string str2, string str3){



  if (str1.length() +  str2.length() != str3.length()){
    return false;
  }


  while (str3.length() != 0){

    int i = 0;
    if (str1[i] == str3[i]){
      i++;
    }

    else if (str2[i] == str3[i]){
      i++;
    }
    else{
      return false; 
    }
    
  }
  
}

int main(){

  string str1 = "abc";
  string str2 = "def";
  string str3= "dabcef";

  cout << CombineStrings(str1, str2, str3);
  
  


}
  
