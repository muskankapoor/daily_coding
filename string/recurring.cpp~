#include<iostream>
#include<unordered_set>
using namespace std;

string RepeatCharacter(string str){
  unordered_set s(str.begin(), str.end());
  for (auto i: str){
    if (!(str.count(i))){
      return i;
      break;
    }
    else{
      continue; 
    }
  }
  return "none";
}
int main(){

  string str = "ABCA";
  cout << RepeatCharacter(str);

}
