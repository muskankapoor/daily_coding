#include <iostream>
#include <string>
using namespace std;

string reverse(string str){


  string reverse_str = "";
  
  for (int i = str.length() - 1; i >= 0; i--){

    reverse_str.push_back(str[i));
    

  }
  return reverse_str;

}


int main(){

  string str = "bold";
  cout << reverse(str);
  
  
}
