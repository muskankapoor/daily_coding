 #include <algorithm>
#include <string>
 #include <iostream>
using namespace std;


string solution(string &s, int k) {
    
    string str = " ";
    
    // 5 4 3 2
    for (int i = s.length()-1; i >= 0; i+=k){
        str = str.substr(i, i - k -1);
    }
    
    return str;
    
}

int main(){

  string str = "2-4A0r7-4k";

  solution(str);


}
