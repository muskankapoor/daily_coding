
int fibonacci(int n) {
    // Complete the function.
    if (n == 0){
        return 0;
    }
    if (n == 1){
        return 1; 
    }
    else{
        return fibonacci(n - 1) + fibonacci(n-2);
    }
    
}

int fib(int n) { 
 
  int f[n+1]; 
  int i; 
  
  /* 0th and 1st number of the series are 0 and 1*/
  f[0] = 0; 
  f[1] = 1; 
  
  for (i = 2; i <= n; i++) { 
      /* Add the previous 2 numbers in the series 
         and store it */
      f[i] = f[i-1] + f[i-2]; 
  } 
  
  return f[n]; 
} 