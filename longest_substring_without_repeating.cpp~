#include <iostream>
#include <map>
#include <string>

using namespace std;

int hashfunction(string str){

  map<int, int> gquiz1; 
  int key;
  int count; 
  int table_size = 26;
  for (int i = 0; i < str.length(); i++){

    key = str[i] % table_size;
    gquiz1.insert(pair<int, int>(key, i));

    if(!gquiz1->contains(key)){
	count++;
      }
    else{
      break;
    }
  }

  return count;

}

int main(){

  string str = "abcabcbb";
  cout << hashfunction(str);

}
