#include <iostream>
#include <stack>
#include <vector>
using namespace std;

// Evaluate Reverse Polish Notation

int Evaluate(int x1, int x2, char s){

  int x3;
  if (s == '+'){
    x3 = x1 + x2;
  }
  else if (s == '-'){
    x3 = x1 - x2;
  }
  else if (s == '/'){
    x3 = x1/x2;
  }
   else if (s == '*'){
    x3 = x1*x2;
  }

  return x3;

}


int ReversePolish(vector<string> arr){
  
  stack<int> s;
  for (int i = 0; i < arr.size(); i++){
    if (isdigit(arr[i])){
      s.push(arr[i]);
    }
    else{
      int x2 = s.pop();
      int x1 = s.pop();
      int x3 = Evaluate(x1, x2, arr[i]);
      s.push(x3);
    }
  }
  return s.top();
}


int main(){

  vector<string> s;
  s.push_back("2");
  s.push_back("1");
  s.push_back("+")
  s.push_back("3");
  s.push_back("*");

  cout << ReversePolish(s);
  
  

	  

}
