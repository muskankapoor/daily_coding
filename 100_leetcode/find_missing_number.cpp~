// find missing number in an array
#include <iostream>
#include <vector>
#include <algorithmn> 
using namespace std;


void missingNumber(vector<int>& nums) {
  std::sort (nums.begin(), nums.begin()+nums.size());
  for (int i = 0; i < nums.size(); i++){
    cout << nums[i];
  }
}

int main(){

  vector<int> num;
  num.push_back(2);
  num.push_back(1);
  num.push_back(3);
  num.push_back(4);
  missingNumber(num):
  

}
