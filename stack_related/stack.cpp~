#include <iostream>
#include <stack>

using namespace std;

class Stacker{
private:
  stack<int> s; 
  int val;
  int current_max = 0; 
public:
  void push(val){
    s.push(val);
  }
  void pop(){
    s.pop();
  }
  int top(){
    return s.top();
  }

  int current(current_max){
    return current_max; 
  }

  
};





int main(){

  Stacker s;
  s.push(1);
  s.push(2);
  s.push(3);

 
  
}
