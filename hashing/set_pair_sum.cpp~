#include <iostream>
#include <set>

using namespace std;

// Given an integer array, output all pairs that sum up to a specific value k.


void ArrPAIR(vector<int> v, int k){


  set<int> s;
  set<int> output; 

  for (int i = 0; i < v.size(); i++){
    int target = k - v[i];


    if (!s.find(target)){
      s.insert(target);
      
    }
    else{
      output.insert(target);
      output.insert(v[i]);
      
    }
  }
  for (auto it= output.begin(); it != output.end(); ++it) {
        cout << ' ' << *it;
  }
   
}
